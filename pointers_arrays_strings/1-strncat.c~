#include "main.h"
#include <stdio.h>
#include <string.h>

/**
* _strncat - the function concatenates two strings
* @dest: pointeur destination
* @src: pointeur source
* Return: pointeur on the string dest
*/

char *_strncat(char *dest, char * src, int n)
{
char  *ptr;
ptr = dest;
while (*dest != '\0')
{
dest ++;
}
while (n > 0 && *src != '\0')
{
*dest = *src;
dest++;
src++;
n--;
}
*dest = '\0';
return(ptr);
}
